{
  "name": "cms-demo",
  "version": "0.0.1",
  "description": "CMS Demo Site",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules",
    "db": "mongod",
    "db:clean": "sudo rm -rf /data/db && sudo mkdir /data/db && sudo chmod -R 777 /data/db",
    "dev": "node bin/start-all-servers",
    "start": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "fs-extra": "^4.0.0",
    "jquery": "^3.2.1",
    "keystone": "^4.0.0-beta.5",
    "lru-cache": "^4.1.1",
    "mongodb": "^2.2.30",
    "octicons": "^5.0.1",
    "vue": "^2.3.4",
    "vue-router": "^2.6.0",
    "vue-server-renderer": "^2.3.4",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.5.2",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "forever-monitor": "^1.7.1",
    "json-server": "^0.10.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.set": "^4.3.2",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^2.11.0",
    "postcss-icss-values": "^2.0.1",
    "postcss-import": "^10.0.0",
